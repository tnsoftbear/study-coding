{
  "name": "tailwindcss_example_2",
  "version": "0.0.1",
  "description": "playing with flowbite",
  "main": "public/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tailwindcss -i ./src/css/main.css -o ./public/dist/main.css",
    "build:watch": "npx tailwindcss -i ./src/css/main.css -o ./public/dist/main.css --watch",
    "install": "npx cpx ./node_modules/flowbite/dist/flowbite.min.js ./public/dist/ && npm run build"
  },
  "keywords": [
    "tailwindcss",
    "flowbite"
  ],
  "author": "Thomas Mayne Reid",
  "license": "MIT",
  "devDependencies": {
    "cpx": "^1.5.0",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "flowbite": "^2.2.1"
  }
}
